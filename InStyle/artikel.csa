/*== BLOG PAGER ==*/
.blog-pager-older-link, .blog-pager-newer-link{margin:40px 10px 20px 10px;}
.blog-pager-newer-link{float:left; display:flex; align-items:center; padding:4px 10px; border-radius:100px; background:#2196F3; color:#fff; 10px; font-size:20px;}
.blog-pager-older-link{float:right; display:flex; align-items:center; padding:4px 10px; border-radius:100px; background:#2196F3; color:#fff; 10px; font-size:20px;}
.span{font-size:10px; display:flex; align-items:center;}
