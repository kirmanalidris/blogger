al-icons .icon a {  color: #fff;  width: 60px;  height: 60px;  font-size: 28px;  line-height: 60px;  text-align: center;  border-radius: 30px;}.pf-wrap .social-icons .icon:nth-of-type(1) a {  background: linear-gradient(to bottom right, #2196F3, #2196F3);}.pf-wrap .social-icons .icon:nth-of-type(2) a {  background: linear-gradient(to bottom right, #2196F3,  #2196F3);}.pf-wrap .social-icons .icon:nth-of-type(3) a {  background: linear-gradient(to bottom right, #2196F3, #2196F3);}.pf-wrap .social-icons .icon h4 {  color: #080911;  font-size: 1em;  margin-top: 1.3em;  margin-bottom: .2em;}.pf-wrap .social-icons .icon p {  color: inherit;  font-size: 12px;}.concept {  position: absolute;  bottom: 25px;  color: inherit;  font-size: .9em;  font-weight: 400;}.concept a {  color: #ac1966;  text-decoration: none;}
