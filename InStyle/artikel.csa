/*== HIDDEN ==*/
.post-footer, #HTML1, #FeaturedPost1{display: none;}

/*== ARTIKEL ==*/
.kirman-post-title-con{height:300px; text-align:center; background:#2196F3; width:calc(100 - 40px); padding:20px;}
.kirman-post-title-con .kirman-img-title{display:none; width:calc(100% - 0px); height:auto;}
.kirman-post-title-con .kirman-post-title{text-transform:uppercase; font-size:1.6em; font-weight:600; color:#fff;}
.post-body{position:relative; background:#fff; padding:20px 10px 10px 10px; margin:-200px 10px 10px 10px; border-radius:10px;}
.post-body .kirman-author{width:40px; height:40px; margin:10px 10px 40px 10px; border-radius:100px; border:2px solid #2196F3;}
.post-body .kirman-name-author{display:none;}
.post-body .kirman-comment-count{color:#fff; font-size:10px; background:#2196F3; border-radius:100px; padding:2px 10px; position: absolute; top:40px; left:80px;}
.post-body img{border-radius:10px; width: 100%; height:auto; margin-left: -15px;}
.separator:nth-child(1){}/*gunakan display:none; untuk hapus gambar pertama*/
/*== ARTIKEL ==*/

/*== BLOCKQUOTE ==*/
.post-body blockquote{border-radius:4px; margin:0; padding:10px; backgroun:#fafafa;}
/*== BLOCKQUOTE ==*/

/*== HIGHLIGHT ==*/
pre .line-number {-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none}pre{background-color:#fff;padding:0 1em;white-space:pre;word-wrap:normal;overflow:auto}pre .line-number,pre code{display:block;font:normal normal 13px/24px Consolas,&quot;Bitstream Vera Sans Mono&quot;,&quot;Courier New&quot;,Courier,monospace!important;color:#069}pre .line-number{float:left;margin:0 1em 0 -1em;color:#aaa;background-color:#f5f5f5;border-right:3px solid #2196f3;text-align:right;min-width:2.5em}pre .line-number span{display:block;padding:0 .5em 0 1em}pre .line-number span:nth-child(even){background-color:#eee}pre .cl{display:block;clear:both}pre{-webkit-user-select:all;-moz-user-select:all;-ms-user-select:all;user-select:all}
/*== HIGHLIGHT ==*/
  
  
/*== IKLAN PARALAX ==*/
.paralax_div{position:relative;overflow:visible;width:300px;height:600px;margin-right:20px;display:inline-block;float:left;z-index:9999}
.paralax_div >div{overflow:hidden;width:100%;height:100%;margin:0;position:absolute;top:0;left:0;clip:rect(auto auto auto auto)}
.paralax_div >div >div{width:100%;height:100%;position:fixed;top:100px;margin:0 auto;-moz-transform:translateZ(0);-webkit-transform:translateZ(0);-ms-transform:translateZ(0);-o-transform:translateZ(0);transform:translateZ(0)}
.paralax_div >div >div >div{width:100%;height:100vh;position:absolute;left:50%;top:0;border:none;-moz-transform:translateX(-50%);-webkit-transform:translateX(-50%);-ms-transform:translateX(-50%);-o-transform:translateX(-50%);transform:translateX(-50%);display:-webkit-flex;display:-ms-flexbox;display:flex;-webkit-align-content:center;align-content:center;-webkit-align-items:center;-ms-flex-align:center;align-items:center;background:#00000000}
.paralax_div >div >div >div >*{margin:0;margin-top:0}
.paralax_div >div >div >div >a{width:100%;height:100vh}
.paralax_div img,.paralax_div iframe,.paralax_div ins{height:600px;width:300px}
.clear{clear:both;display:block}
@media screen and (max-width:640px){.paralax_div{width:100%;height:600px;margin:0 auto;float:none}.paralax_div >div >div >div{justify-content:center;display:flex;margin:auto;width:300px;height:600px;left:50%;-moz-transform:translateX(-50%);-webkit-transform:translateX(-50%);-ms-transform:translateX(-50%);-o-transform:translateX(-50%);transform:translateX(-50%)}.paralax_div >div >div{width:100%;left:0;text-align:center}.paralax_div img{margin:0 auto;width:auto;max-width:100%;height:auto;border-radius:0;height:600px}}
@media screen and (max-width:320px){.paralax_div iframe,.parallax_banner ins{margin:0 auto;width:100%;height:600px}}

/*== IKLAM PARALAX ==*/

